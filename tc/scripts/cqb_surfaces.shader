/////////////////////////////////////water/////////////
textures/surfparms/water
{
	nocompress
	qer_editorimage textures/surfparms/water.tga
	qer_trans 0.75
	q3map_globaltexture

	surfaceparm water
	surfaceparm trans
	surfaceparm nonsolid
	surfaceparm lightfilter

	tessSize 256
	cull disable
	deformVertexes wave 12 sin 0 2 .5 .25
	{ 
		map textures/surfparms/waterb.tga
		rgbGen identity
		blendFunc blend
		alphaGen const 0.75
		depthWrite
		//tcmod scale .5 .5
		tcmod turb 0 0.01 0 0.15
		tcmod scroll .01 .01
	}
	{ 
		map textures/surfparms/waterc.tga
		rgbGen identity
		blendFunc blend
		alphaGen const 0.75
		depthWrite
		//tcmod scale .5 .5
		tcmod turb 0 0.08 0 0.18
		tcmod scroll .08 .08
	}
	{
		map $lightmap
		blendFunc GL_DST_COLOR GL_ZERO
		rgbgen identity
		depthFunc equal	
	}
}

