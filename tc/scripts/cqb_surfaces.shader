//////////////////////sky////////////////////////
textures/surfparms/skyparam
{
    nocompress
	qer_editorimage textures/surfparms/sky.tga
	surfaceparm noimpact
	surfaceparm nolightmap
	surfaceparm sky
	//q3map_sun 1 0.95 0.65 10000 140 16
	q3map_sunExt 1 0.95 0.65 5000 190 45 10 45
	q3map_lightmapFilterRadius 0 8
	q3map_skylight 500 10
	q3map_nofog	

	skyparms  textures/surfparms/puresky 512 -
	sunshader textures/coroner_skies/full_moon3


	{
		fog off
		clampmap textures/coroner_skies/sky5n_mask.tga
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		tcMod scale 0.956 0.956
		tcMod transform 1 0 0 1 -1 -1
		tcMod rotate 0.25 0.25
		rgbGen const ( 0.4 0.4 0.45 )
		alphaGen const 1.0
	}
}

