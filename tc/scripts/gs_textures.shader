textures/goldmann/wood_beech
{       
   qer_editorimage textures/goldmann/wood_beech.tga
   surfaceparm woodsteps
   
   {
      map $lightmap
      rgbGen identity
   }
   {
      map textures/goldmann/wood_beech.tga
      blendFunc GL_DST_COLOR GL_ZERO
      rgbGen identity
   }
}

