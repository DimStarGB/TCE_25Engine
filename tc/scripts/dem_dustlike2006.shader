textures/dem_dustlike2006/dsbox1
{       
	qer_editorimage textures/dem_dustlike2006/dsbox1.tga
	surfaceparm mat_wood
	
	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/dem_dustlike2006/dsbox1.tga
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

