models/bruce_foliage/plant1
{
	qer_alphafunc greater 0.5
	
	cull disable
	surfaceparm trans
	surfaceparm nomarks
	surfaceparm alphashadow
	surfaceparm nolightmap
	
        nomipmaps
        nopicmip
	
	deformVertexes wave 256 sin 0 1 0.25 0.1
        {
		map models/bruce_foliage/plant1.tga
		alphaFunc GE128
		rgbGen vertex
	}
}
