gfx/levelshots/dt_aa_box
{ 
	nopicmip 
	nocompress 	
	nomipmaps
	{ 
      	map gfx/levelshots/dt_aa_box.tga 
      	depthfunc equal 
      	blendfunc blend 
      	rgbGen vertex 
      	alphaGen vertex 
	} 
}
